    "lorem-ipsum": "^1.0.3",
    "marked": "^0.3.6",
    "morgan": "^1.7.0",
    "node-noop": "^1.0.0",
    "normalize.css": "^4.2.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-transition-group": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "smoothscroll": "^0.2.2",
    "vhost": "^3.0.2",
    "webfontloader": "^1.6.26",
    "whatwg-fetch": "^1.0.0"
