{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "docs#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "src/generated/**",
        "public/typedoc/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "types/**"]
    },
    "lint": {},
    "lint-fix": {},
    "format": {},
    "check-format": {},
    "typecheck": {},
    "test": {
      "interactive": true,
      "cache": false
    },
    "test-run": {},
    "test-coverage": {
      "interactive": true,
      "cache": false,
      "outputs": ["coverage/**"]
    },
    "test-run-coverage": {
      "outputs": ["coverage/**"]
    },
    "test-run-snapshot": {},
    "clean": {
      "cache": false
    },
    "clean-dist": {
      "cache": false
    }
  }
}
