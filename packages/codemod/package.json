{
  "name": "@react-md/codemod",
  "version": "6.0.2",
  "description": "ReactMD codemod scripts",
  "type": "module",
  "bin": {
    "rmd-codemod": "./dist/index.js"
  },
  "exports": {
    "./transforms/*": "./dist/transforms/*.js",
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "scripts": {
    "prepare-release": "npm-run-all clean-dist build",
    "clean-dist": "rm -rf dist",
    "clean": "rm -rf .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "test-run": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest",
    "test": "pnpm test-run --watch",
    "test-run-coverage": "pnpm test-run --coverage",
    "test-run-snapshot": "pnpm test-run -u",
    "build": "swc -d ./dist --strip-leading-paths src",
    "build-watch": "pnpm run build --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlaursen/react-md.git",
    "directory": "packages/codemod"
  },
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "keywords": [
    "react-md",
    "material design",
    "react",
    "codemod",
    "jscodeshift",
    "transforms"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@inquirer/confirm": "^6.0.3",
    "@inquirer/input": "^5.0.3",
    "@inquirer/rawlist": "^5.1.0",
    "@inquirer/search": "^4.0.3",
    "commander": "^14.0.2",
    "glob": "^13.0.0",
    "jscodeshift": "^17.3.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "postcss-value-parser": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@jest/globals": "^30.2.0",
    "@jest/types": "^30.2.0",
    "@react-md/eslint-config": "workspace:*",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/jscodeshift": "^17.3.0",
    "@types/node": "^24.10.4",
    "eslint": "^9.39.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "volta": {
    "node": "24.11.1",
    "pnpm": "10.24.0"
  }
}
