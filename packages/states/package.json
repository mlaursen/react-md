{
  "name": "@react-md/states",
  "version": "2.0.0-alpha-1",
  "description": "A package for managing the different interaction states on elements like hover, focus, and pressed",
  "scripts": {
    "build:commonjs": "tsc -p tsconfig.commonjs.json",
    "build:modules": "tsc -p tsconfig.modules.json",
    "build:definitions": "tsc -p tsconfig.modules.json -d --declarationDir ./types",
    "build": "npm-run-all -p \"build:commonjs\" \"build:modules\" \"build:definitions\" \"styles\"",
    "clean": "rimraf es lib types dist",
    "prebuild": "npm run clean",
    "update-definitions": "rimraf types && npm run build:definitions",
    "watch:definitions": "watch \"npm run update-definitions\" src --filter ../../tsWatchFilter.js",
    "styles": "copyfiles -u 1 \"src/**/*.scss\" dist",
    "watch:styles": "watch \"npm run styles\" src --filter ../../scssWatchFilter.js",
    "watch": "npm-run-all -p \"build:commonjs -- --watch\" \"build:modules -- --watch\" \"watch:styles\"",
    "watch:with-defs": "npm-run-all -p watch watch:definitions",
    "test": "jest src",
    "docs": "doc-generator",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "es/*",
    "lib/*",
    "types/*",
    "dist/*"
  ],
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "repository": "https://github.com/mlaursen/react-md",
  "license": "MIT",
  "dependencies": {
    "@react-md/theme": "^2.0.0-alpha-1",
    "@react-md/transition": "^2.0.0-alpha-1",
    "classnames": "^2.2.6",
    "memoize-one": "^4.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/jest": "^23.1.0",
    "@types/memoize-one": "^3.1.1",
    "@types/prop-types": "15.5.3",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "copyfiles": "^2.0.0",
    "doc-generator": "^2.0.0-alpha-1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.1.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "tslint": "^4.5.1",
    "typescript": "^2.9.2",
    "watch": "^1.0.2"
  }
}
