{
  "name": "docs-generator",
  "private": true,
  "version": "0.0.18",
  "description": "MDX Plugins for the documentation site",
  "type": "module",
  "exports": {
    "./*": "./dist/*.js"
  },
  "scripts": {
    "dev": "pnpm run watch",
    "watch": "npm-run-all --parallel build-esm-watch build-types-watch",
    "build": "npm-run-all build-esm build-types",
    "build-esm": "swc -d ./dist --strip-leading-paths src",
    "build-esm-watch": "pnpm run build-esm --watch",
    "build-types": "tsc -P tsconfig.types.json",
    "build-types-watch": "pnpm run build-types --watch",
    "prism-themes": "tsx src/scripts/create-prism-themes.ts",
    "clean-dist": "rm -rf dist",
    "clean": "rm -rf .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "test-run": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest",
    "test": "pnpm test-run --watch",
    "test-run-coverage": "pnpm test-run --coverage",
    "test-run-snapshot": "pnpm test-run -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "@prettier/sync": "^0.6.1",
    "@react-md/code": "workspace:*",
    "@react-md/core": "workspace:*",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/estree-jsx": "^1.0.5",
    "@types/hast": "^3.0.4",
    "@types/unist": "^3.0.3",
    "estree-util-value-to-estree": "^3.5.0",
    "github-slugger": "^2.0.0",
    "glob": "^13.0.0",
    "gray-matter": "^4.0.3",
    "hast-util-heading-rank": "^3.0.0",
    "hast-util-properties-to-mdx-jsx-attributes": "^1.0.1",
    "lz-string": "^1.5.0",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-mdx-jsx": "^3.2.0",
    "mdast-util-to-string": "^4.0.0",
    "micromark-extension-mdxjs": "^3.0.0",
    "next": "^16.1.3",
    "postcss": "^8.5.6",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-remove-prefixes": "^1.2.0",
    "postcss-selector-parser": "^7.1.1",
    "postcss-sorting": "^9.1.0",
    "prettier": "^3.8.0",
    "rehype": "^13.0.2",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "sass": "^1.97.2",
    "sucrase": "^3.35.1",
    "ts-morph": "^27.0.2",
    "unified": "^11.0.5",
    "unist-util-is": "^6.0.1",
    "unist-util-mdx-define": "^1.1.2",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.2",
    "vfile": "^6.0.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@mdx-js/mdx": "^3.1.1",
    "@react-md/eslint-config": "workspace:*",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.10.4",
    "@types/postcss-sorting": "^8.0.2",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "cssnano-preset-default": "^7.0.10",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "npm-run-all2": "^8.0.2",
    "pretty-ms": "^9.3.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.30.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "volta": {
    "node": "24.12.0",
    "pnpm": "10.28.0"
  }
}
