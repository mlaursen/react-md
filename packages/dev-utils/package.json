{
  "name": "@react-md/dev-utils",
  "version": "3.0.0-alpha.0",
  "description": "Common build utilities for react-md. Contains helpers for building, watching, testing, etc",
  "private": true,
  "bin": {
    "dev-utils": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rimraf bin dist",
    "executable": "node executable.js",
    "compile": "tsc",
    "build": "npm-run-all compile executable",
    "watcher": "chokidar \"bin/cli.js\" -c \"npm run executable\"",
    "watch": "npm-run-all -p \"compile -w\" watcher"
  },
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@octokit/core": "^3.5.1",
    "chokidar": "^3.5.2",
    "commander": "^8.0.0",
    "cpx": "^1.5.0",
    "cssnano": "^5.0.6",
    "dotenv": "^10.0.0",
    "filesize": "^7.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "gzip-size": "^6.0.0",
    "he": "^1.2.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.7.1",
    "markdown-toc": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.2",
    "pretty-ms": "^7.0.0",
    "prompts": "^2.4.1",
    "rimraf": "^3.0.2",
    "sass": "^1.36.0",
    "sassdoc": "^2.7.2",
    "ts-morph": "^11.0.3",
    "typedoc": "^0.21.4",
    "typedoc-plugin-sourcefile-url": "^1.0.6",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/cssnano": "^4.0.1",
    "@types/he": "^1.1.2",
    "@types/prompts": "^2.0.14",
    "@types/sass": "^1.16.1",
    "chokidar-cli": "^2.1.0"
  }
}
