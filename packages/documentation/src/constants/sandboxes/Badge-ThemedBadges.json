  "src/Demo.tsx": {
    "content": "import React, { FC } from \"react\";\nimport { BadgedButton, BadgeTheme } from \"@react-md/badge\";\n\nimport styles from \"./SimpleExamples.module.scss\";\n\nconst themes: BadgeTheme[] = [\"primary\", \"secondary\", \"default\", \"clear\"];\n\nconst Demo: FC = () => (\n  <>\n    {themes.map((theme) => (\n      <BadgedButton\n        key={theme}\n        id={`badged-button-${theme}`}\n        badgeTheme={theme}\n        className={styles.container}\n      >\n        {theme.length}\n      </BadgedButton>\n    ))}\n  </>\n);\n\nexport default Demo;\n",
    "isBinary": false
  },
  "src/SimpleExamples.module.scss": {
    "content": ".container {\n  margin: 1rem;\n}\n",
    "isBinary": false
  }
}
