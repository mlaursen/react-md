{
  "name": "documentation",
  "version": "1.1.0-alpha-3",
  "description": "The documentaiton website for react-md",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "prebuild": "rimraf public/assets",
    "build": "webpack --progress --colors --env.production",
    "start": "cross-env NODE_ENV=production node index.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "repository": "https://github.com/mlaursen/react-md",
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-system-import-transformer": "^3.1.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cross-env": "^5.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "helmet": "^3.6.1",
    "hpp": "^0.2.2",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.10",
    "qs": "^6.4.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.1.3",
    "react-hot-loader": "3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-api-middleware": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.3",
    "shallowequal": "^1.0.1",
    "smooth-scroll": "cferdinandi/smooth-scroll",
    "webpack-isomorphic-tools": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-jsx-a11y": "4",
    "eslint-plugin-react": "6",
    "extract-text-webpack-plugin": "2",
    "file-loader": "^0.11.1",
    "image-webpack-loader": "^3.3.1",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.5",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  },
  "jest": {
    "timers": "fake",
    "globals": {
      "__DEV__": true,
      "__CLIENT__": true
    },
    "setupFiles": [
      "<rootDir>/src/beforeTest.js"
    ],
    "moduleDirectories": [
      "<rootDir>/src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "react-md/(.*)": "<rootDir>/../$1"
    }
  }
}
